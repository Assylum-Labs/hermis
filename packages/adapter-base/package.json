      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@hermis/solana-headless-core": "^1.0.7",
    "@solana-mobile/wallet-adapter-mobile": "2.0.0",
    "@wallet-standard/base": "^1.1.0",
    "bs58": "^6.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [],
  "author": "Mctursh",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.5.2",
  "devDependencies": {
    "@hermis/solana-headless-format-config": "^1.0.5",
    "@types/node": "^22.13.8",
    "tsup": "^8.0.0"
  }
}
