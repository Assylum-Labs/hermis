{
  "name": "@agateh/solana-headless-core",
  "version": "1.2.0",
  "description": "Core wallet logic for the Solana Headless SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "pnpm run build",
    "clean": "rm -rf dist"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@noble/ed25519": "^2.2.3",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/web3.js": "^1.98.0"
  },
  "devDependencies": {
    "@agateh/solana-headless-format-config": "workspace:*",
    "@types/node": "^22.13.8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/__tests__/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.[jt]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "keywords": [],
  "author": "mctursh",
  "license": "ISC",
  "packageManager": "pnpm@10.5.2"
}
