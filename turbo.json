{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "packages/*/dist/**", "apps/*/dist/**"]
    },
    "build:pages": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "apps/*/dist/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    }
  }
}
